    <children>
        <Label layoutX="115.0" layoutY="14.0" text="Add Customer">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label layoutX="14.0" layoutY="84.0" text="ID">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="14.0" layoutY="113.0" text="First name">
            <font>
                <Font size="14.0" />
            </font>
