<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="controllers.common.MenuController" stylesheets="/common/common.css">
    <top>
        <HBox id="tabHBox">
            <Button onAction="#openTodayTab" text="Today"/>
            <Button onAction="#openCustomersTab" text="Customers"/>
            <Button onAction="#openBookingsTab" text="Bookings"/>
            <Button onAction="#openVehiclesTab" text="Vehicles"/>
            <Button onAction="#openPartsTab" text="Parts"/>
            <Button onAction="#openSRCTab" text="Specialist Repairs"/>
            <Button fx:id="SRCButton" onAction="#openSRCManagementTab" text="Repair Centers"/>
            <Button fx:id="UsersButton" onAction="#openUsersTab" text="Users"/>
            <Button fx:id="logoutButton" id="logoutButton" onAction="#logout" text="Logout"/>
        </HBox>
    </top>
    <center>
    </center>
</BorderPane>
